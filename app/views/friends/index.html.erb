<p style="color: green"><%= notice %></p>

<h1>Friends</h1>

<div id="friends">
<table class="table" >
  <% @friends.each do |<tr>friend</tr>| %>
    <%= render friend %>
    <p>
      <%= link_to "Show this friend", friend %>
    </p>
  <% end %>
</div>

<%= link_to "New friend", new_friend_path %>
